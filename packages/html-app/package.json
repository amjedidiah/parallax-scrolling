  "homepage": "https://amjedidiah.github.io/parallax-scrolling",
  "scripts": {
    "prebuild": "pnpm nx run nextjs-app:tailwind-eject",
    "build": "mv ../nextjs-app/build.css ./src/tailwind.css",
    "start": "pnpm parcel src/index.html",
    "dev": "pnpm parcel src/index.html",
    "publish": "gh-pages -d src",
    "submit": "zip -r submission.zip src/ && mv submission.zip ~/Desktop/submission.zip && echo \"Copied submission.zip to Desktop\" && cd ~/Desktop && unzip -o submission.zip -d submission && open submission/src"
  },
